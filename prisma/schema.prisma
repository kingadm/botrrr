datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}



model License {
  id        String   @id @default(cuid())
  key       String   @unique
  hwid      String?
  banned    Boolean  @default(false)
  expiresAt DateTime?
  plan      String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
